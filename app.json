{
  "expo": {
    "name": "stockams",
    "slug": "stockams",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "center",
      "backgroundColor": "#FEF9B0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flowentech.stockams",
      "config": {
        "googleSignIn": {
          "reservedClientId": "your-client-id"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.flowentech.stockams"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "locals":{
      "en":"./Apps/translation/en.json"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "94c62b25-dcdb-4f4b-b91d-a100e27eced3"
      }
    }
  }
}
